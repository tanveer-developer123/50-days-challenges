<%- include("partials/header", { title: "Contact Us", activePage: "contact" }) %>

<main>
  <h1>Contact Us</h1>
  <form>
    <label>Name:</label>
    <input type="text" placeholder="Enter your name" required>
    <br><br>

    <label>Email:</label>
    <input type="email" placeholder="Enter your email" required>
    <br><br>

    <label>Message:</label>
    <textarea placeholder="Write your message"></textarea>
    <br><br>

    <button type="submit" id="alertBtn">Send Message</button>
  </form>
</main>

<%- include("partials/footer") %>
